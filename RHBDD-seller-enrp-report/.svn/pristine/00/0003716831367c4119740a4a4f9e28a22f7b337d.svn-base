# Data source settings
datasource.driverClassName=oracle.jdbc.OracleDriver
#datasource.url=jdbc:oracle:thin:@//172.16.30.136:1521/orcl
datasource.url=jdbc:oracle:thin:@localhost:1521:xe
datasource.username=FPXAPP
datasource.password=FPXAPP

# Settings for Spring
spring.jmx.enabled=false
spring.main.banner-mode=off

# Settings for Spring Batch
spring.batch.initializer.enabled=false
batch.repository.isolation_level_for_create=ISOLATION_READ_COMMITTED

# Store the downloaded CFT files in this directory
app.local.cft.root.directory=target/local_NAS/merchant_cft

# logging level
logging.level.org.springframework.context.annotation.ConfigurationClassEnhancer=ERROR
logging.level.com.jcraft=WARN
logging.level.org.mockftpserver=WARN